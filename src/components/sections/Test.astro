        <Card
            title="Card 1"
            description="Tailored strategies"
            icon="Plus"
            buttonLabel="Contact Us"
            buttonUrl="/contact"
            alignment="left"
        />
        <Card
            title="Card 2"
            description="Optimized workflows"
            icon="Check"
            buttonLabel="Learn More"
            buttonUrl="/learn-more"
            alignment="right"
        />
        <Card
            title="Card 3"
            description="Data-driven insights"
            icon="Chart"
            buttonLabel="Get Started"
            buttonUrl="/get-started"
            alignment="center"
        />
