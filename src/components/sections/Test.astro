<!-- Team / Founders Section -->
<Section id="team" title="Team" subtitle="Founders" centered={true}>
    <PeopleGrid persons={people} />
    <Notice type="info" title="Information"
        >This is a demo site. The team members shown are fictional.</Notice
    >
    <Notice type="warning" title="Warning"
        >This is a demo site. The team members shown are fictional.</Notice
    >
    <Notice type="success" title="Success"
        >This is a demo site. The team members shown are fictional.</Notice
    >
    <Notice type="error" title="Error"
        >This is a demo site. The team members shown are fictional.</Notice
    >
